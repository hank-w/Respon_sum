{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "stats": {
      "type": "object",
      "properties": {
        "numCorrect": {
          "type": "integer"
        },
        "numIncorrect": {
          "type": "integer"
        },
        "numUnresponded": {
          "type": "integer"
        }
      },
      "required": ["numCorrect", "numIncorrect", "numUnresponded"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
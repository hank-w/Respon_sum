{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "id": "http://jsonschema.net",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "studentNumber": {
      "type": "string"
    },
    "institution": {
      "type": "string"
    },
    "performance": {
      "type": "object",
      "properties": {
        "numCorrect": {
          "type": "integer"
        },
        "numIncorrect": {
          "type": "integer"
        },
        "numUnresponded": {
          "type": "integer"
        }
      },
      "required": ["numCorrect", "numIncorrect", "numUnresponded"],
      "additionalProperties": false
    }
  },
  "required": ["id", "name", "studentNumber", "institution", "performance"],
  "additionalProperties": false
}
